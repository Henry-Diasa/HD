{
  "name": "hd",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/Henry-Diasa/HD#readme",
  "bugs": {
    "url": "https://github.com/Henry-Diasa/HD/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Henry-Diasa/HD.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build:site": "cross-env SITE_BUILD_ENV=build && rimraf doc-dist && dumi build",
    "commit": "git-cz",
    "deploy:site": "npm run build:site && gh-pages -d doc-dist",
    "dev": "dumi dev",
    "preview:site": "cross-env SITE_BUILD_ENV=PREVIEW && npm run build:site && serve doc-dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@umijs/fabric": "^2.14.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^2.1.13",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.0",
    "typescript": "^4.9.5"
  }
}
